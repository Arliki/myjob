<!--{if $show_asynclist}-->
get_asynclist("{:url('exchange/asynclist_list', array('page'=>$this->_var['page'], 'sort'=>$this->_var['sort'], 'order'=>$this->_var['order']))}" , '__TPL__/images/loader.gif');
<!--{/if}-->
</script>
</body>
</html>
